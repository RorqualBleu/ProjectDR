<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.server.mapper.SupervisorMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.server.pojo.Supervisor">
        <id column="tel_id" property="telId" />
        <result column="password" property="password" />
        <result column="real_name" property="realName" />
        <result column="birthday" property="birthday" />
        <result column="sex" property="sex" />
        <result column="remarks" property="remarks" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        tel_id, password, real_name, birthday, sex, remarks
    </sql>

</mapper>
